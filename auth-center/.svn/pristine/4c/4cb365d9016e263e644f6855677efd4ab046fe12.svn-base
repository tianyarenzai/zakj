.small_banner a span{
	font-size:16px;
	line-height:160px;
	display:block;
	height:50%;
	cursor:pointer;
    text-align: center;
    color: #666;
}
.small_banner a em{
	padding-left:60px;
	line-height:30px;
	display:block;
	text-align:center;
	font-size:12px;
	line-height:71px;
}
.small_banner_list a:hover{
	text-decoration:none;
	color:#333;
}
.small_banner_list label{
    color: #666;
    position: absolute;
    top: 75px;
    left: 0;
    width: 85px;
    z-index: 100;
    background: #fff;
    height: 35px;
}

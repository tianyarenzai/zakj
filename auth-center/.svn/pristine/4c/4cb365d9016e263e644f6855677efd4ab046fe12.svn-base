
.small_banner{
	width:1000px;
	margin:0 auto;
}
.small_banner a{
	display:inline-block;
	width:84px;
	height:100px;
	font-size:14px;
	float:left;
	position:relative;
	overflow:hidden;
    text-align: center;
    z-index: 85;
}
.small_banner a b{
	position:absolute;
	width:100%;
	height:200%;
	top:0px;
	left:0px;
	display:inline-block;
	background:url(../images/small_banner_bg.png) no-repeat 0 0;
	z-index:8;
}

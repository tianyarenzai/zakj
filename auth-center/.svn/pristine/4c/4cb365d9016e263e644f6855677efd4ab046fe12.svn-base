.small_banner_box{
    position: relative;
    width:1000px;
    margin:0 auto;
    height: 120px;
}
.small_banner{
    position: relative;
	width:930px;
    margin:0 auto;
    height: 120px;
    overflow:hidden;
}
.left,
.right{
    position: absolute;
    top:50px;
    border: none;
    width: 13px;
    height: 24px;
    cursor: pointer;
}
.left{
    background: url(../images/left.png) no-repeat 0 0;
    left: 10px;
}
.right{
    background: url(../images/right.png) no-repeat 0 0;
    right: 10px;
}
.small_banner_list{
    position: absolute;
    margin:20px auto;
    /*width: 930px;*/
    overflow: hidden;
    height: 100px;
    left: 0;
    top:0;
}


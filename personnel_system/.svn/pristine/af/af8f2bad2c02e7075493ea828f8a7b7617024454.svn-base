package com.zakj.security.common.beans;

/**
 * String类型ajax响应封装，
 * 注意：这里不是返回JSON串，如果要返回一个对象有JSON串可以使用AjaxBeanResult
 * @see com.zajy.security.common.beans.AjaxBeanResult
 * @author 0ops
 *
 */
public class AjaxStringResult extends AjaxBasicResult {

	private String data;

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

}
